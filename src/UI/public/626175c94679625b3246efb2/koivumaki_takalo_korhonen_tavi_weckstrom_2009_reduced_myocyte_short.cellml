   <component name="calcium_buffering">
      <variable initial_value="70" name="LTRPN_tot" public_interface="out" units="uM"/>
      <variable initial_value="0.1" name="kltrpn_pos" public_interface="out" units="per_uM_per_msec"/>
      <variable initial_value="0.06" name="kltrpn_neg" public_interface="out" units="per_msec"/>
      <variable name="J_LTRPN" public_interface="out" units="uM_per_msec"/>
      <variable name="B_i" public_interface="out" units="dimensionless"/>
      <variable name="CaM_tot" public_interface="in" units="uM"/>
      <variable name="Km_CaM" public_interface="in" units="uM"/>
      <variable name="Ca_cyt" public_interface="in" units="uM"/>
      <variable name="LTRPN" public_interface="in" units="uM"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>B_i</ci>
            <apply>
               <power/>
               <apply>
                  <plus/>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
