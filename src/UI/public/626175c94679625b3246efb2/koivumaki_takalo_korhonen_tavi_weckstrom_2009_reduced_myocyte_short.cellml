                  <ci>time</ci>
               </bvar>
               <ci>Ca_cyt</ci>
            </apply>
            <apply>
               <times/>
               <ci>B_i</ci>
               <apply>
                  <minus/>
                  <apply>
                     <plus/>
                     <apply>
                        <minus/>
                        <apply>
                           <divide/>
                           <apply>
                              <times/>
                              <ci>INaCa</ci>
                              <ci>Acap</ci>
                              <ci>C_m</ci>
                           </apply>
                           <apply>
                              <times/>
                              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                              <ci>V_myo</ci>
                              <ci>F</ci>
                           </apply>
                        </apply>
                        <ci>J_cyt_serca</ci>
                     </apply>
                     <ci>J_leak</ci>
                     <ci>Ca_input</ci>
                  </apply>
                  <ci>J_LTRPN</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>LTRPN</ci>
            </apply>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>kltrpn_pos</ci>
                  <ci>Ca_cyt</ci>
                  <apply>
                     <minus/>
                     <ci>LTRPN_tot</ci>
                     <ci>LTRPN</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>kltrpn_neg</ci>
                  <ci>LTRPN</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>PLB_dephosph</ci>
            </apply>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>kplb_pos</ci>
                  <apply>
                     <minus/>
                     <ci>PLB_tot</ci>
                     <ci>PLB_dephosph</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>kplb_neg</ci>
                  <apply>
                     <power/>
                     <apply>
                        <plus/>
                        <ci>CaMKII_reg</ci>
                        <ci>PKA_reg</ci>
                     </apply>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
                  </apply>
                  <ci>PLB_dephosph</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Ca_serca</ci>
            </apply>
            <apply>
               <minus/>
               <ci>J_cyt_serca</ci>
               <ci>J_serca_sr</ci>
            </apply>
         </apply>
      </math>
