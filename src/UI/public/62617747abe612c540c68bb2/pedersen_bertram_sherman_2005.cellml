						<ci>JGPDH</ci>
					</apply>
				</apply>
			</apply>
			
			<apply>
				<eq/>
				<ci>fback</ci>
				<apply>
					<plus/>
					<ci>r</ci>
					<ci>y</ci>
				</apply>
			</apply>
		</math>
	</component>

