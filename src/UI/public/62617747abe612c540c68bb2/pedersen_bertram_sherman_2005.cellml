	<component name="Jglut" cmeta:id="Jglut">
		<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
			<rdf:Description rdf:about="#Jglut">
				<dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">Jglut</dc:title>
				<dcterms:alternative xmlns:dcterms="http://purl.org/dc/terms/">rate of GLUT-2 facilitated glucose transport</dcterms:alternative>
			</rdf:Description>
		</rdf:RDF>
		<variable name="Jglut" units="millimolar_per_millisecond" public_interface="out"/>
		<variable name="Kglut" units="millimolar" initial_value="7"/>
		<variable name="Vglut" units="millimolar_per_millisecond" initial_value="8"/>
		<variable name="Gi" units="millimolar" public_interface="in"/>
		<variable name="Ge" units="millimolar" public_interface="in"/>
		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply>
				<eq/>
				<ci>Jglut</ci>
				<apply>
					<times/>
					<ci>Vglut</ci>
					<apply>
						<divide/>
						<apply>
					        <times/>
						    <apply>
							    <minus/>
							    <ci>Ge</ci>
							    <ci>Gi</ci>
						    </apply>
						    <ci>Kglut</ci>
					    </apply>
					    <apply>
						    <times/>
						    <apply>
							    <plus/>
							    <ci>Kglut</ci>
							    <ci>Ge</ci>
						    </apply>
						    <apply>
							    <plus/>
							    <ci>Kglut</ci>
							    <ci>Gi</ci>
						    </apply>
						</apply>
					</apply>
				</apply>
			</apply>
		</math>
	</component>

	<component name="Jgk" cmeta:id="Jgk">
		<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
			<rdf:Description rdf:about="#Jgk">
				<dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">Jgk</dc:title>
				<dcterms:alternative xmlns:dcterms="http://purl.org/dc/terms/">rate of the glucokinase reaction</dcterms:alternative>
			</rdf:Description>
		</rdf:RDF>
		<variable name="Jgk" units="millimolar_per_millisecond" public_interface="out"/>
		<variable name="Kgk" units="millimolar" initial_value="7"/>
		<variable name="Vgk" units="millimolar_per_millisecond" initial_value="0.8"/>
		<variable name="ngk" units="dimensionless" initial_value="4"/>
		<variable name="Gi" units="millimolar" public_interface="in"/>
		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply>
				<eq/>
				<ci>Jgk</ci>
				<apply>
					<divide/>
					<apply>
						<times/>
						<ci>Vgk</ci>
						<apply>
							<power/>
							<ci>Gi</ci>
							<ci>ngk</ci>
						</apply>
					</apply>
					<apply>
						<plus/>
						<apply>
							<power/>
							<ci>Kgk</ci>
							<ci>ngk</ci>
						</apply>
						<apply>
							<power/>
							<ci>Gi</ci>
							<ci>ngk</ci>
						</apply>
					</apply>
				</apply>
			</apply>
		</math>
	</component>

	<component name="JGPDH" cmeta:id="JGPDH">
		<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
