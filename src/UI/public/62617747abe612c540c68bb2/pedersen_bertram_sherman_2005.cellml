	<component name="m" cmeta:id="m">
		<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
			<rdf:Description rdf:about="#m">
				<dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">m</dc:title>
