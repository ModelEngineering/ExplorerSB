		<variable name="kappa" units="dimensionless" public_interface="in"/>
		<variable name="JPFK" units="micromolar_per_millisecond" public_interface="in"/>
		<variable name="Jgk" units="millimolar_per_millisecond" public_interface="in"/>
		<variable name="time" units="millisecond" public_interface="in"/>
		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply>
				<eq/>
				<apply>
					<diff/>
					<bvar>
						<ci>time</ci>
					</bvar>
					<ci>G6P</ci>
				</apply>
				<apply>
					<times/>
					<ci>kappa</ci>
					<apply>
						<minus/>
						<ci>Jgk</ci>
						<ci>JPFK</ci>
					</apply>
				</apply>
			</apply>
		</math>
