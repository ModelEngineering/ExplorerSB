	<component name="Ge" cmeta:id="Ge">
		<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
			<rdf:Description rdf:about="#Ge">
				<dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">Ge</dc:title>
				<dcterms:alternative xmlns:dcterms="http://purl.org/dc/terms/">extracellular glucose</dcterms:alternative>
			</rdf:Description>
		</rdf:RDF>
		<variable name="Ge" units="millimolar" public_interface="out"/>
		
		<variable name="gluper" units="dimensionless" initial_value="7.0"/>
		<variable name="gluamp" units="millimolar" initial_value="0.0"/>
		<variable name="glubasa1" units="millimolar" initial_value="7.0"/>
		<variable name="glustep" units="dimensionless" initial_value="0.0"/>
		<variable name="steptime" units="millisecond" initial_value="480000"/>
		<variable name="GeStep" units="millimolar"/>
		
		<variable name="time" units="millisecond" public_interface="in"/>
		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply>
				<eq/>
				<ci>Ge</ci>
				<apply>
					<plus/>
					<ci>glubasa1</ci>
					<apply>
					    <times/>
					    <ci>gluamp</ci>
						<apply>
							<minus/>
							<cn cellml:units="dimensionless">1</cn>
							<ci>glustep</ci>
				        </apply>
						<apply>
							<cos/>
							<apply>
						        <divide/>
								<apply>
					                <times/>
					                <ci>time</ci>
									<cn cellml:units="dimensionless">2</cn>
									<pi/>
			                    </apply>
								<apply>
					                <times/>
					                <cn cellml:units="millisecond">60000</cn>
									<ci>gluper</ci>
								</apply>
							</apply>
						</apply>
					</apply>
					<ci>GeStep</ci>
				</apply>
			</apply>
			
			<apply>
				<eq/>
				<ci>GeStep</ci>
				<piecewise>
				    <piece>
					    <apply>
					        <times/>
					        <ci>glustep</ci>
					        <ci>gluamp</ci>
					    </apply>
					    <apply>
						    <gt/>
						    <apply>
							    <divide/>
							    <apply>
							        <minus/>
							        <ci>time</ci>
								    <ci>steptime</ci>
						        </apply>
							    <cn cellml:units="millisecond">1000.0</cn>
						    </apply>
							<cn cellml:units="dimensionless">1.0</cn>
					    </apply>
				    </piece>
					<piece>
					    <apply>
					        <times/>
							<apply>
							    <divide/>
							    <apply>
							        <minus/>
							        <ci>time</ci>
								    <ci>steptime</ci>
						        </apply>
							    <cn cellml:units="millisecond">1000.0</cn>
						    </apply>
					        <ci>glustep</ci>
					        <ci>gluamp</ci>
					    </apply>
						<apply>
						    <and/>
						    <apply>
						        <gt/>
								<apply>
							        <divide/>
							        <apply>
							            <minus/>
							            <ci>time</ci>
								        <ci>steptime</ci>
						            </apply>
							        <cn cellml:units="millisecond">1000.0</cn>
						        </apply>
								<cn cellml:units="dimensionless">0.0</cn>
					        </apply>
							<apply>
						        <lt/>
								<apply>
							        <divide/>
							        <apply>
							            <minus/>
							            <ci>time</ci>
								        <ci>steptime</ci>
						            </apply>
							        <cn cellml:units="millisecond">1000.0</cn>
						        </apply>
								<cn cellml:units="dimensionless">1.0</cn>
					        </apply>
						</apply>
				    </piece>
					<piece>
					    <cn cellml:units="millimolar">0.0</cn>
					    <apply>
						    <lt/>
						    <apply>
							    <divide/>
							    <apply>
							        <minus/>
							        <ci>time</ci>
								    <ci>steptime</ci>
						        </apply>
							    <cn cellml:units="millisecond">1000.0</cn>
						    </apply>
							<cn cellml:units="dimensionless">0.0</cn>
					    </apply>
				    </piece>
				</piecewise>
			</apply>
		</math>
	</component>

