   <component xmlns="http://www.cellml.org/cellml/1.0#" name="environment">
      <variable name="time" units="day" public_interface="out"/>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="glucose_dynamics">
      <variable name="G" units="mg_per_dl" initial_value="600" public_interface="out"/>
      <variable name="R0" units="mg_per_dl_per_day" initial_value="846"/>
      <variable name="SI" units="ml_per_microU_per_day" initial_value="0.72"/>
      <variable name="EG0" units="per_day" initial_value="1.44"/>
      <variable name="time" units="day" public_interface="in"/>
      <variable name="I" units="microU_per_ml" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>G</ci>
            </apply>
            <apply>
               <minus/>
               <ci>R0</ci>
               <apply>
                  <times/>
                  <apply>
                     <plus/>
                     <ci>EG0</ci>
                     <apply>
                        <times/>
                        <ci>SI</ci>
                        <ci>I</ci>
                     </apply>
                  </apply>
                  <ci>G</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="insulin_dynamics">
      <variable name="I" units="microU_per_ml" initial_value="0" public_interface="out"/>
      <variable name="alpha" units="mg2_per_dl2" initial_value="2000"/>
      <variable name="sigma" units="microU_per_ml_per_day" initial_value="43.2"/>
      <variable name="k" units="per_day" initial_value="432"/>
      <variable name="time" units="day" public_interface="in"/>
      <variable name="G" units="mg_per_dl" public_interface="in"/>
      <variable name="beta" units="mg" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>I</ci>
            </apply>
            <apply>
               <minus/>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <apply>
                        <divide/>
                        <ci>beta</ci>
                        <cn cellml:units="mg">1</cn>
                     </apply>
                     <ci>sigma</ci>
                     <apply>
                        <power/>
                        <ci>G</ci>
                        <cn cellml:units="dimensionless">2</cn>
                     </apply>
                  </apply>
                  <apply>
                     <plus/>
                     <ci>alpha</ci>
                     <apply>
                        <power/>
                        <ci>G</ci>
                        <cn cellml:units="dimensionless">2</cn>
                     </apply>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>k</ci>
                  <ci>I</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component xmlns="http://www.cellml.org/cellml/1.0#" name="beta_cell_dynamics">
      <variable name="beta" units="mg" initial_value="0" public_interface="out"/>
      <variable name="d0" units="per_day" initial_value="0.06"/>
      <variable name="r1" units="dl_per_mg_per_day" initial_value="0.84e-3"/>
      <variable name="r2" units="dl2_per_mg2_per_day" initial_value="0.24e-5"/>
      <variable name="time" units="day" public_interface="in"/>
      <variable name="G" units="mg_per_dl" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>beta</ci>
            </apply>
            <apply>
               <times/>
               <apply>
                  <plus/>
                  <apply>
                     <times/>
                     <ci>r1</ci>
                     <ci>G</ci>
                  </apply>
                  <apply>
                     <minus/>
                     <ci>d0</ci>
                  </apply>
                  <apply>
                     <times/>
                     <apply>
                        <minus/>
                        <ci>r2</ci>
                     </apply>
                     <apply>
                        <power/>
                        <ci>G</ci>
                        <cn cellml:units="dimensionless">2</cn>
                     </apply>
                  </apply>
               </apply>
               <ci>beta</ci>
            </apply>
         </apply>
      </math>
   </component>
