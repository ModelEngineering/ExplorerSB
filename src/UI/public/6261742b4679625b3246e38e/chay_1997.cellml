                  </apply>
                  <apply>
                     <minus/>
                     <ci>Ca_lum</ci>
                     <ci>Ca_i</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>k_pump</ci>
                  <ci>Ca_i</ci>
               </apply>
            </apply>
         </apply>
      </math>
